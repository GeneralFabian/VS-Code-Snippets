	"2D Vector": {
		
    "prefix": "2vec",
		"body": "\\begin{pNiceMatrix} $1 \\\\\\\\ $2 \\end{pNiceMatrix}",
		"description": "2D Vector in one line"
	},
	"2D Vector side": {
		
    "prefix": "2vecs",
		"body": "\\begin{pNiceMatrix} $1 & $2 \\end{pNiceMatrix}",
		"description": "2D Vector in one line"
	},
	"2D Vector long": {
		
    "prefix": "2vecm",
		"body": [
			"\\begin{pNiceMatrix}",
			"\t$1 \\\\\\\\",
			"\t$2",
			"\\end{pNiceMatrix}"
		],
		"description": "2D Vector in multiple lines"
	},

{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"textforall": {
		"prefix": "@taa",
		"body": "\\tforall "
	},
	"textexists": {
		"prefix": "@tee",
		"body": "\\texists "
	},
	"Inline math": {
		"prefix": "inm",
		"body": "\\( $1 \\)"
	},
	"No brace expansion": {
		"prefix": "{",
		"body": "",
		"description": "No brace expansion"
	},
	"Mathtext": {
		"prefix": "MTE",
		"body": "\\text{${1:...}}",
		"description": "Insert mathtext"
	},
	"Set": {
		"prefix": "BSE",
		"body": "\\Set*{$1\\given $2}",
		"description": "Menge mit skaliertem | "
	},
	"Begin Something": {
		"prefix": "beg",
		"body": [
			"\\begin{${1:...}}",
			"\t$2",
			"\\end{${1:...}}",
		],
		"description": "Insert an environment to be specified"
	},
	"Begin Proof": {
		"prefix": "BPR",
		"body": [
			"\\begin{proof}[$1]",
			"\t$2",
			"\\end{proof}"
		],
		"description": "Insert a proof environment"
	},
	"Begin Remark Starred": {
		"prefix": "BSRE",
		"body": [
			"\\begin{remark*}",
			"\t$1",
			"\\end{remark*}"
		],
		"description": "Insert a remark* environment"
	},
	"Begin Blank Starred": {
		"prefix": "BSBL",
		"body": [
			"\\begin{blank*}[$1]",
			"\t$2",
			"\\end{blank*}"
		],
		"description": "Insert a blank* environment"
	},
	"Begin Blank": {
		"prefix": "BBL",
		"body": [
			"\\begin{blank}[$1]",
			"\t$2",
			"\\end{blank}"
		],
		"description": "Insert a blank environment"
	},
	"Begin Blank Starred (Italics)": {
		"prefix": "BSBLI",
		"body": [
			"\\begin{blankit*}[$1]",
			"\t$2",
			"\\end{blankit*}"
		],
		"description": "Insert a blank* environment"
	},
	"Begin Blank (Italics)": {
		"prefix": "BBLI",
		"body": [
			"\\begin{blankit}[$1]",
			"\t$2",
			"\\end{blankit}"
		],
		"description": "Insert a blank environment"
	},
	"Begin Subroof": {
		"prefix": "bspr",
		"body": [
			"\\begin{subproof}[$1]",
			"\t$2",
			"\\end{subproof}"
		],
		"description": "Insert a subproof environment"
	},
	"Begin Figure": {
		"prefix": "bfi",
		"body": [
			"\\begin{figure}[H]",
			"\t\\centering",
			"\t\\includegraphics[width=${4:0.5}\\linewidth]{$1}",
			"\t\\caption${2:*}{$3}",
			"\t\\label{fig:$1}",
			"\\end{figure}"
		],
		"description": "Insert a figure with caption and label"
	},
	"Inline Graphic": {
		"prefix": "inlgr",
		"body": "\\includegraphics[height=\\baselineskip]{$1}",
		"description": "Insert a graphic at lineheight"
	},
